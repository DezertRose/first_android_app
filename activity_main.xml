<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <fragment
        android:id="@+id/fragment_input"
        android:name="com.example.lr_2.Fragment_input"
        android:layout_width="405dp"
        android:layout_height="424dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toTopOf="@+id/fragment_output"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_input" />

    <fragment
        android:id="@+id/fragment_output"
        android:name="com.example.lr_2.Fragment_output"
        android:layout_width="404dp"
        android:layout_height="274dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_input"
        tools:layout="@layout/fragment_output" />

</androidx.constraintlayout.widget.ConstraintLayout>